# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to GitHub - it's in .gitignore

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Health API Configuration
HEALTH_API_KEY=your_health_api_key_here
HEALTH_API_BASE_URL=https://api.nlm.nih.gov/medlineplus/v2/

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Python Backend URL (for production, use your Vercel URL)
PYTHON_BACKEND_URL=http://localhost:8000

# Additional Security
SECRET_KEY=your_secret_key_for_session_management
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.vercel.app